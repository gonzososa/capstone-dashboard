{
    "trailingSlash": "auto",
    "routes":[
        {
            "route": "/api*",
            "methods": ["GET"],
            "allowedRoles": ["anonymous"]
        }
    ],
    "responseOverrides": {
        "401": {
            "rewrite": "/401.html",
            "statusCode": 401
        },
        "404":{
            "rewrite": "/404.html",
            "statusCode": 404
        }
    }
}